<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://vps-dcat.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>vps-dcat.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>vps-dcat</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$vps-dcat</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DCAT" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:oebr8fvh6i.database.windows.net,1433;Initial Catalog=vps-dcat_db;User ID=vps.admin@oebr8fvh6i;Password=VPPSpassw0rd!@#" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=.;Initial Catalog=PViMS;User ID=AppOwner;Password=appapp" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\DCAT_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=.;Initial Catalog=PViMS;User Id=AppOwner; Password=appapp; MultipleActiveResultSets=True;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DCAT-Web.config Connection String">
      <ParameterValue>data source=tcp:oebr8fvh6i.database.windows.net,1433;initial catalog=vps-dcat_db;user id=vps.admin@oebr8fvh6i;password=VPPSpassw0rd!@#;MultipleActiveResultSets=True</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>