@using System.Web.Optimization
<!DOCTYPE html>
<html lang="en">
<head>
    <title>data service Tests</title>
    @Styles.Render("~/Content/css", "~/Content/less")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" type="text/css" href="qunit/qunit.css" />
</head>
<body>
    <h1 id="qunit-header">Dcat dataservice Tests..</h1>
    <h2 id="qunit-banner"></h2>
    <div id="qunit-testrunner-toolbar"></div>
    <h2 id="qunit-userAgent"></h2>
    <ol id="qunit-tests"></ol>
    <div id="qunit-fixture"></div>

    @Scripts.Render("~/bundles/jquery")
    <script type="text/javascript" src="~/Scripts/Tests/qunit/qunit.js"></script>
    @Scripts.Render("~/Scripts/knockout-3.3.0.js")
    <script src="~/Scripts/require.js" type="text/javascript"></script>

    <script type="text/javascript">
        window.defineSaved = window.define;

        window.define = function (a, b, c) {
            a === 'dataservice' ? window.testFn = c : defineSaved(a, b, c);
        };
    </script>

    <script type="text/javascript" src="~/Scripts/Dexie.js"></script>
    <script type="text/javascript" src="~/Scripts/uuid.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.mockjax.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.mockjson.js"></script>
    <script type="text/javascript" src="~/Scripts/knockout/dataservice.js"></script>
    <script type="text/javascript" src="~/Scripts/knockout/dataservice.patient.js"></script>
    <script type="text/javascript" src="~/Scripts/knockout/db.js"></script>
    <script type="text/javascript" src="~/Scripts/knockout/config.js"></script>
    <script src="main.tests.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/Tests/vm.dataservice.tests.js"></script>

    <script type="text/javascript">
        require(['vm-dataservice-tests-function']);
    </script>
</body>
</html>
